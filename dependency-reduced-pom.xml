<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.awsblog.queueing</groupId>
  <artifactId>awsblog-queueing</artifactId>
  <version>1.0.0</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.6.0</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
          <encoding>UTF-8</encoding>
          <forceJavacCompilerUse>true</forceJavacCompilerUse>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.0.0</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.12</version>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>hamcrest-core</artifactId>
          <groupId>org.hamcrest</groupId>
        </exclusion>
      </exclusions>
    </dependency>
  </dependencies>
  <properties>
    <aws-java-lambda-core.version>1.2.0</aws-java-lambda-core.version>
    <apache-commons-logging.version>1.2</apache-commons-logging.version>
    <junit.version>4.12</junit.version>
    <jsii-runtime.version>0.14.3</jsii-runtime.version>
    <aws-java-lambda-log4j.version>1.1.0</aws-java-lambda-log4j.version>
    <aws-java-sdk.version>1.11.608</aws-java-sdk.version>
    <log4j.version>2.11.2</log4j.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <cdk.version>1.3.0.DEVPREVIEW</cdk.version>
    <jackson.version>2.9.9</jackson.version>
    <jdk.version>1.8</jdk.version>
    <aws-java-lamba-events.version>2.2.6</aws-java-lamba-events.version>
  </properties>
</project>

